{
  "compilerOptions": {
    "target": "ESNext",
    "useDefineForClassFields": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "strict": true,
    "jsx": "react-jsx",
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "@formFields/*": ["src/components/forms/fields/*"],
      "@covernoteField/*": ["src/components/forms/covernoteField/*"],
      "@app/*": ["src/app/*"]

    }
  },
  "include": ["src"]
}
